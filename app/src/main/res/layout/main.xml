<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="16dp"
    android:layout_marginTop="16dp"
    android:layout_marginBottom="16dp"
    android:layout_marginRight="16dp"
    android:orientation="vertical"
    tools:context="com.tablesoft.tecache.MainActivity">


    <Button
        android:id="@+id/botonini"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Muerete"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/presence_video_online"
        android:layout_marginBottom="27dp"
        android:layout_above="@+id/botonini"
        android:layout_toEndOf="@+id/botonini" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/presence_video_busy"
        android:layout_alignTop="@+id/imageButton"
        android:layout_toStartOf="@+id/botonini" />
    
</RelativeLayout>
